{
  "name": "sanity-plugin-iconify",
  "version": "1.1.1",
  "description": "Icon picker based on Iconify",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/waspeer/sanity-plugin-iconify#readme",
  "bugs": {
    "url": "https://github.com/waspeer/sanity-plugin-iconify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/waspeer/sanity-plugin-iconify.git"
  },
  "license": "MIT",
  "author": "Wannes Salom√© <mail@wannessalome.nl>",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "pnpm run generate-types && plugin-kit link-watch",
    "lint": "eslint .",
    "prepublish-only": "run-s build",
    "watch": "pkg-utils watch --strict",
    "generate-types": "node src/lib/generate-types",
    "prepare": "husky install"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@iconify/react": "^4.1.1",
    "@iconify/utils": "^2.1.22",
    "@sanity/icons": "^2.11.4",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.9.3",
    "@tanstack/react-query": "^5.28.6",
    "change-case": "^5.4.3",
    "ts-pattern": "^5.0.8",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/load": "^19.2.0",
    "@iconify/json": "^2.2.195",
    "@iconify/types": "^2.0.0",
    "@sanity/pkg-utils": "^5.1.4",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@tanstack/react-query-devtools": "^5.28.6",
    "@types/react": "^18.2.69",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.35.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3",
    "styled-components": "^5 || ^6"
  },
  "engines": {
    "node": ">=14"
  },
  "pnpm": {
    "overrides": {
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "postcss@<8.4.31": ">=8.4.31",
      "follow-redirects@<1.15.4": ">=1.15.4",
      "follow-redirects@<=1.15.5": ">=1.15.6"
    }
  },
  "sanityPlugin": {
    "verifyPackage": {
      "scripts": false
    }
  }
}
