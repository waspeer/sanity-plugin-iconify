{
  "name": "sanity-plugin-iconify",
  "version": "2.0.0",
  "description": "Icon picker based on Iconify",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/waspeer/sanity-plugin-iconify#readme",
  "bugs": {
    "url": "https://github.com/waspeer/sanity-plugin-iconify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/waspeer/sanity-plugin-iconify.git"
  },
  "license": "MIT",
  "author": "Wannes Salom√© <mail@wannessalome.nl>",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "pnpm run generate-types && plugin-kit link-watch",
    "lint": "eslint .",
    "prepublish-only": "run-s build",
    "watch": "pkg-utils watch --strict",
    "generate-types": "node src/lib/generate-types",
    "prepare": "husky",
    "dev": "tsx dev/vite.ts"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "dependencies": {
    "@iconify/react": "^5.0.2",
    "@iconify/utils": "^2.1.32",
    "@sanity/icons": "^3.4.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.8.9",
    "@tanstack/react-query": "^5.55.0",
    "change-case": "^5.4.4",
    "downshift": "^9.0.8",
    "ts-pattern": "^5.3.1",
    "use-debounce": "^10.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/load": "^19.4.0",
    "@iconify/json": "^2.2.245",
    "@iconify/types": "^2.0.0",
    "@sanity/pkg-utils": "^6.11.0",
    "@sanity/plugin-kit": "^4.0.18",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@tanstack/eslint-plugin-query": "^5.53.0",
    "@tanstack/react-query-devtools": "^5.55.0",
    "@types/react": "^18.3.5",
    "@types/styled-components": "^5.1.34",
    "@waspeer/config": "^2.3.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "rimraf": "^6.0.1",
    "sanity": "^3.57.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3",
    "styled-components": "^6"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^9",
      "micromatch@<4.0.8": ">=4.0.8"
    }
  },
  "sanityPlugin": {
    "verifyPackage": {
      "scripts": false,
      "eslintImports": false
    }
  }
}
